<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="souza.home.com.mypersonalcarassistant">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">

        <activity android:name=".listaVeiculos.ui.Extras.DicasActivity"></activity>
        <activity android:name=".listaVeiculos.ui.Extras.CTBActivity" />
        <activity android:name=".listaVeiculos.ui.Extras.DirecaoDefensivaActivity" />
        <activity android:name=".listaVeiculos.ui.onboarding.MainOnboarding" />
        <activity android:name=".listaVeiculos.ui.onboarding.OnboardingActivity" />
        <activity android:name=".listaVeiculos.ui.LoginActivity" />
        <activity android:name=".listaVeiculos.ui.OpenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".listaVeiculos.ui.TransitionActivity" />
        <activity android:name=".listaVeiculos.ui.MaintenanceActivity" />
        <activity
            android:name=".listaVeiculos.ui.MenuActivity"
            android:label="@string/title_activity_menu" />
        <activity android:name=".listaVeiculos.ui.CadastroActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>